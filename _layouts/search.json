[
  {% if page.target == "home" %}
    {%- assign posts = site.posts -%}
  {% else %}
    {%- assign posts = site.posts -%}
  {% endif %}
  {%- for subPost in posts -%}
  {
      "title": "{{ subPost.title | xml_escape }}",
      "url":"{{ subPost.url | xml_escape }}"
  }
  {%- unless forloop.last -%},{%- endunless -%}
  {%- endfor -%}
]